{
  "id": "manager",
  "name": "Manager",
  "description": "Workflow stage orchestration, gate enforcement, and escalation routing.",
  "version": "1.0.0",
  "role": "MANAGER",
  "render": {
    "engine": "mustache",
    "targets": [
      {
        "source": "SOUL.md",
        "destination": "workspace/agents/{{agentSlug}}/SOUL.md"
      },
      {
        "source": "HEARTBEAT.md",
        "destination": "workspace/agents/{{agentSlug}}/HEARTBEAT.md"
      },
      {
        "source": "MEMORY.md",
        "destination": "workspace/agents/{{agentSlug}}/MEMORY.md"
      },
      {
        "source": "overlay.md",
        "destination": "workspace/agents/{{agentSlug}}.md"
      }
    ]
  },
  "provisioning": {
    "enabled": true,
    "steps": [
      "create_files",
      "register_agent"
    ]
  },
  "author": "clawcontrol",
  "tags": [
    "clawcontrol",
    "default"
  ],
  "teamDefaults": {
    "reportsTo": null,
    "delegatesTo": [
      "research",
      "plan",
      "plan_review",
      "build",
      "build_review",
      "ui",
      "ui_review",
      "security",
      "ops"
    ],
    "receivesFrom": [
      "research",
      "plan",
      "plan_review",
      "build",
      "build_review",
      "ui",
      "ui_review",
      "security",
      "ops"
    ],
    "canMessage": [
      "research",
      "plan",
      "plan_review",
      "build",
      "build_review",
      "ui",
      "ui_review",
      "security",
      "ops"
    ],
    "capabilities": {
      "canDelegate": true,
      "canSendMessages": true
    }
  }
}
